	<Folder>
		<name>Find a Geographical Name in BC</name>
		<visibility>0</visibility>
		<Snippet maxLines="0"></Snippet>
		<description><![CDATA[<html>
        
          <head>
            <meta http-equiv="Content-Type" content="text/xml">
          </head>
            
          <body>
           <form action="http://apps.gov.bc.ca/pub/bcgnws/names/official/soundlike?" target="Soundlike Search" method="get" id="Soundlike Search">
	  
            <table border="0" cellspacing="0" cellpadding="0" name="Table 2" width ="300">
               <input type="hidden" name="startIndex" value="0" id="startIndex"></input>
               <input type="hidden" name="itemsPerPage" value="20" id="itemsPerPage"></input>
               <input type="hidden" name="maxResults" value="1000" id="maxResults"></input>
               <input type="hidden" name="minScore" value="0.5" id="minScore"></input>
               <input type="hidden" name="outputStyle" value="summary" id="outputFormat"></input>
               <input type="hidden" name="outputSRS" value="4326" id="outputSRS"></input>
               <input type="hidden" name="asAttachment" value="0" id="asAttachment"></input>
               <input type="hidden" name="embed" value="0" id="embed"></input>
               <input type="hidden" name="featureClass" value="*" id="featureClass"></input>
               <input type="hidden" name="featureCategory" value="*" id="featureCategory"></input>
               <input type="hidden" name="outputFormat" value="kml" id="outputFormat"></input>
               
               <tr>
                 <td>
                  <input name="name" type="text" id="name" size="37" wrap="virtual"></textarea> 
                 </td>
                 <td>
                  <input type="submit" value="Search"></input>
                 </td>
               </tr>      

            </table>

           </form>   
          </body>
          
     </html>]]></description>
	</Folder>

    <Folder>
      <name>Find an Address</name>
      <visibility>0</visibility>
      <Snippet maxLines="0"></Snippet>
      <description><![CDATA[<html>
      
        <head></head>
      
        <body>
              
             <form action="http://apps.gov.bc.ca/pub/geocoder/addresses.kml" target="geocode" method="get" id="geocode" autocomplete="on">
        
                 <table width="63%" border="0" cellpadding="4" cellspacing="0">
                    <tr>
                      <td align="left" colspan="3">
                        Address:
                      </td>
                    </tr>
                    <tr>
                      <td>
                        <input type="text" name="addressString" id="addressString" size="45"></input>
                      </td>
                    </tr>
                    <tr>
                      <td colspan="3">
                        <b>Examples:</b><br>
				   <i>525 Superior Street, Victoria, BC</i><br>
                           <i>301-780 Blanshard St, Victoria, BC</i><br>
                           <i>Johnson St and Government St, Victoria, BC</i><br>
                      </td>
                    </tr>
                    <tr>
                      <td>                	
                        <b>Preferences:</b>
                      </td>
                    </tr>
                    <tr>
                      <td colspan="3">
                        Setback (in metres): <input type="text" name="setBack" id="setBack" value="0"></input>
                      </td>
                    </tr>
                    <tr>
                      <td colspan="3">
                        Min Score (0-100): <input type="text" name="minScore" id="minScore" value="0"></input>
                      </td>
                    </tr>
                    <tr>
                      <td colspan="3">
                        Max Results: <input type="text" name="maxResults" id="maxResults" value="1"></input>
                      </td>
                    </tr>

			<tr>
                      <td colspan="3">
					Point Type: <select name="locationDescriptor">
						<option value="any" selected>Any</option>
						<option value="accessPoint">Access</option>
						<option value="frontDoor">Front Door</option>
						<option value="parcelPoint">Parcel</option>
						<option value="rooftopPoint">Rooftop</option>
						<option value="routingPoint">Routing</option>
					</select>
				</td>
			</tr>

                    <tr>
                      <td colspan="3">
                        Interpolation: <input type="radio" name="interpolation" value="adaptive" checked="checked">Adaptive </input>
                                             <input type="radio" name="interpolation" value="linear">Linear </input>
                                             <input type="radio" name="interpolation" value="none">None </input>
                      </td>
                    </tr>
                    <tr>
                      <td colspan="3">
                        Echo unmatched address details: <input type="radio" name="echo" value="true" checked="checked">Yes </input>
                        <input type="radio" name="echo" value="false">No</input>
                      </td>
                    </tr>
                    <tr>
                      <td>
                        <input type="hidden" name="outputSRS" id="outputSRS" value="4326"></input>
                      </td>
                    </tr>
                    <tr>
                      <td>
                        <input type="hidden" name="outputFormat" id="outputFormat" value="kml"></input>
                      </td>
                    </tr>
                    <tr>
                      <td>
                        <input type="submit" value="Geocode"></input>
                      </td>
                    </tr>
               </table>

            </form>

       </body>
      
      </html>
      
      ]]>
      
      </description>
      
    </Folder>


